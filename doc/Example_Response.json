{
    "document": {
        "type": "APLA",
        "version": "0.8",
        "mainTemplate": {
            "parameters": [
                "payload"
            ],
            "item": {
                "type": "Selector",
                "items": [
                    {
                        "when": "${payload.confirm.status == 'Success'}",
                        "type": "Speech",
                        "contentType": "text",
                        "content": "${payload.confirm.string}"
                    },
                    {
                        "when": "${payload.confirm.status == 'Fail'}",
                        "type": "Speech",
                        "contentType": "text",
                        "content": "Test True."
                    },
                    {
                        "type": "Sequencer",
                        "items": [
                            {
                                "type": "Selector",
                                "strategy": "randomItem",
                                "items": [
                                    {
                                        "type": "Speech",
                                        "contentType": "text",
                                        "content": "Default Sentence."
                                    },
                                    {
                                        "type": "Speech",
                                        "contentType": "text",
                                        "content": "Default Alternative Sentence."
                                    }
                                ]
                            },
                            {
                                "type": "Speech",
                                "contentType": "text",
                                "content": "Possible next Sentence."
                            }
                        ]
                    }
                ]
            }
        }
    },
    "datasources": {}
}